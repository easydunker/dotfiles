[user]
    name = Yingyi Hu
    email = yingyi.hu@nauto.com
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
[merge]
    tool = vimdiff
[alias]
    cb  = checkout -b
    cl  = config --list
    br   = branch
    d   = diff
    fu  = fetch upstream
    fo  = fetch origin
    l   = log -n 40 --pretty=oneline
    pm  = push -u origin +master
    por = push -u origin
    pup = push -u upstream
    ri  = rebase -i
    ru  = rebase upstream/master
    ro  = rebase origin/master
    st   = status
    ss  = show staging
    su  = submodule update --init --recursive
[url "git@github.com:"]
    insteadOf = https://github.com/
[filter "lfs"]
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
